		"@types/node-fetch": "^3.0.3",
		"@types/ws": "^8.5.4",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.8.8",
		"tsc-watch": "^6.0.0",
		"typescript": "^4.9.5"
