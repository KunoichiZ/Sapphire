    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "typeorm": "^0.2.29",
    "utf-8-validate": "^5.0.3",
    "ws": "^7.4.1",
    "zlib-sync": "^0.1.7"
  },
  "imports": {
    "#lib/*": "./dist/lib/*.js",
