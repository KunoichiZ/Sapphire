		"@types/common-tags": "^1.8.1",
		"@types/pg": "^8.6.6",
		"bufferutil": "^4.0.7",
		"chokidar": "^3.5.3",
		"colorette": "^2.0.19",
		"common-tags": "^1.8.2",
		"discord-api-types": "^0.37.35",
		"discord.js": "^12.5.3",
