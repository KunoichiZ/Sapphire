  "main": "dist/Sapphire.js",
  "author": "KunoichiZ",
  "license": "Apache-2.0",
  "private": true,
  "keywords": [
    "discord",
    "discordjs",
    "discord-bot",
    "sapphire",
    "pokemon-crossroads",
